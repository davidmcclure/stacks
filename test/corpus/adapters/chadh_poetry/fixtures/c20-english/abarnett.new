<firstl><l ln="1        ">Jug</l></firstl>
<l ln="2        ">&indent;of wine</l>
<l ln="3        ">Health</l>
<l ln="4        ">&indent;is fine</l>
<l ln="5        ">Head</l>
<l ln="6        ">&indent;is mine</l>
<l ln="7        ">Heart</l>
<l ln="8        ">&indent;is thine</l>
<l ln="9        ">An&abheart;y</l>
<l ln="10      ">&indent;old line</l>
<l ln="11      ">Sine</l>
<l ln="12      ">&indent;Valentine</l>
